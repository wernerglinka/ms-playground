{% extends "layout.njk" %}

{% block body %}
  <section class="main-content">

  {{ latestBlogPosts | dump | safe }}
  {{ featuredBlogPosts | dump | safe }}
  {{ allSortedBlogPosts | dump | safe }}
  {{ annualizedBlogPosts | dump | safe }}
  

  <section>
    <h2>All blogposts:</h2>
    <ul>
      {% for post in featuredBlogPosts %}
      <li>
        <a href="/{{post.path}}">{{post.title}}</a>
        <p>{{post.date | blogDate}}
        <p>{{post.author}}</p>
      </li>
      {% endfor%}
    </ul>
  </section>


    <div class="container">
      {{ contents | safe }}

      {{ sections.content | md | safe }}
    </div>
  </section>
{% endblock %}